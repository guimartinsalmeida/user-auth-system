{
  "version": 2,
  "name": "frontend",
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://api-backend.vercel.app"
    }
  }
}
