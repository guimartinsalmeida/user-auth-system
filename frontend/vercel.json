{
  "version": 2,
  "name": "frontend",
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "VITE_API_URL": "https://api-backend.vercel.app"
    }
  }
}
